{
  "name": "smoothly",
  "private": false,
  "version": "0.0.40",
  "description": "Web component library written in Stencil.",
  "license": "MIT",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "unpkg": "dist/smoothly.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint --project .",
    "build": "stencil build",
