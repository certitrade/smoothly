    "prepare": "npm run build",
    "clean": "rm -rf dist doc www .stencil"
  },
  "dependencies": {
    "@stencil/core": "1.0.2",
    "isoly": "0.0.7",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/puppeteer": "1.12.4",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "puppeteer": "1.17.0",
    "tslint": "^5.17.0",
    "workbox-build": "4.3.1"
  }
}
